num1 = 10
num2 = 12
num3 = 13
sum = num1 + num2 + num3
product = num1 * num2 * num3
average = (num1 + num2 + num3) / 3
max_num = max(num1, num2, num3)
print("сумма чисел:", sum)
print("произведение:", product)
print("среднее:", average)
print("max:", max_num)

